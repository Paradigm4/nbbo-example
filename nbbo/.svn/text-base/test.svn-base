iquery -naq "remove(add_orders)" 2>/dev/null
iquery -naq "remove(del_orders)" 2>/dev/null
iquery -naq "remove(orders)" 2>/dev/null

echo REMEMBER THIS SHIT NEEDS TO BE IN A SINGLE CHUNK FOR THE HACK TO WORK!
iquery -naq "store(build(<x:string>[i=1:10,20,0],'A,'+format(i,'%.0f')+','+format((random()%100)/10.0,'%f')+','+format(random()%100,'%.0f')),add_orders)"
iquery -naq "store(build_sparse(<x:string>[i=1:10,20,0],'D,'+format(i,'%.0f')+','+format((random()%100)/10.0,'%f')+','+format(random()%100,'%.0f'),i<6),del_orders)"
iquery -aq "store(concat(add_orders,del_orders),orders)"

iquery -odcsv -aq "variable_window(orders,i,1,0,nbbo(x))"
